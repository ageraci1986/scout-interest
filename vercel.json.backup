{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/server-vercel.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/src/server-vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
